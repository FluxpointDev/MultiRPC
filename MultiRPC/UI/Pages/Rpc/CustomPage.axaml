<page:RpcPage xmlns="https://github.com/avaloniaui"
         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
         xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
         xmlns:rpcUI="clr-namespace:MultiRPC.UI.Pages.Rpc"
         xmlns:rpc="clr-namespace:MultiRPC.Rpc"
         xmlns:page="clr-namespace:MultiRPC.Rpc.Page"
         mc:Ignorable="d"
         d:DesignWidth="800"
         d:DesignHeight="450"
         x:Class="MultiRPC.UI.Pages.Rpc.CustomPage">
    <Border x:Name="brdContent" CornerRadius="15, 15, 0, 0">
        <Grid x:Name="grdContent" RowDefinitions="Auto Auto *">
            <WrapPanel x:Name="wrpProfileSelector" />
            <StackPanel Grid.Row="1" x:Name="stpProfileEditor" Height="40" Orientation="Horizontal"  Margin="0,7">
                <TextBlock FontWeight="SemiBold" x:Name="tblProfileName" Margin="10,0,0,0" VerticalAlignment="Center" />
                <Rectangle Margin="5,2" Fill="White" Width="1" VerticalAlignment="Stretch" />
                <Image x:Name="imgProfileEdit" Opacity="0.6" Width="38" Height="38" />
                <Image x:Name="imgProfileShare" Opacity="0.6" Width="38" Height="38" />
                <Image x:Name="imgProfileAdd" Opacity="0.6" Width="38" Height="38" />
                <Image x:Name="imgProfileDelete" Opacity="0.6" Width="38" Height="38" />
            </StackPanel>
        </Grid>
    </Border>
</page:RpcPage>